{
  "name": "jsjumpstart",
  "version": "1.0.0",
  "description": "Boilerplate JS project to hit the ground running.",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node --debug buildScripts/srcServer.js",
    "security-check": "nsp check",
    "lint": "esw --ignore-path ./.eslintignore webpack.config.* src/**/* buildScripts/**/* --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter nyan buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/server/api/db.json --port 3001"
  },
  "author": "Jonathan Trowbridge",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.26.1",
    "esformatter": "^0.10.0",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.14.2",
    "file-loader": "^0.9.0",
    "flexboxgrid": "^6.3.1",
    "font-awesome": "^4.7.0",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.16.6",
    "method-override": "^2.3.7",
    "mongoose": "^4.7.3",
    "morgan": "^1.7.0",
    "npm-run-all": "^4.0.0",
    "nsp": "^2.6.2",
    "open": "7.0.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "es6-promise": "^4.0.5",
    "esformatter-jsx": "^7.4.0",
    "eslint": "^3.13.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^1.0.1",
    "js-beautify": "^1.6.8",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.4",
    "localtunnel": "^1.8.2",
    "mocha": "^3.2.0",
    "redux-immutable-state-invariant": "^1.2.4",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.13.2"
  }
}
